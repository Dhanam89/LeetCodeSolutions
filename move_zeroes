class Solution(object):
    def moveZeroes(self, nums):
        """
        :type nums: List[int]
        :rtype: None Do not return anything, modify nums in-place instead.
        """
        n = len(nums)
        index_last_non_zero_value = 0
        
        for i in range(n):
            if nums[i] != 0:
                nums[index_last_non_zero_value] = nums[i]
                index_last_non_zero_value += 1

        for i in range(index_last_non_zero_value, n):
            nums[i] = 0
