class Solution(object):
    def isSubsequence(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        i, j = 0, 0

        s_len = len(s)

        if s_len == 0:
            return True

        s_ind = 0

        for char in t:
            if char == s[s_ind]:
                s_ind +=1 
                if s_ind == s_len:
                    return True 

        return False
            

